platform :ios, '9.0'

target 'FireVote' do
  use_frameworks!
  pod 'SwiftyJSON' # Json解析
  pod 'EVReflection' # Json转Model
  pod 'Alamofire' # 网络请求
  pod 'SDWebImage' # 图片加载
  pod 'SnapKit' # 自动布局
  pod 'JPush' # 推送
  pod 'MJRefresh' # 上拉加载
  pod 'UITableView+FDTemplateLayoutCell' # 自适应
  pod 'Reusable' # 复用
  pod 'MBProgressHUD' # HUD
  pod 'TZImagePickerController' #图片选择
  pod 'YBImageBrowser' #图片浏览
  # 多页面嵌套
  pod 'JXCategoryView'
  pod 'JXPagingView/Paging'
  # 键盘
  pod 'IQKeyboardManagerSwift'
  # YY
  pod 'YYText'
  # 腾讯组件库
  pod 'QMUIKit'
end
